<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
<!-- for UH-60 HD -->
  <path>interior2.ac</path>
  
  <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>300</max-m>
    </animation>


  <!-- Pilote -->
  <model>
    <name>Pilot1</name>
    <path>Aircraft/UH-60/Models/Pilot/Helipilot4/pilot.xml</path>
    <offsets>
      <x-m>-6 </x-m>
      <y-m> 0.65 </y-m>
      <z-m> -0.8 </z-m>
    </offsets>
  </model>
  
  <model>
    <name>Pilot2</name>
    <path>Aircraft/UH-60/Models/Pilot/Helipilot4/pilot.xml</path>
    <offsets>
      <x-m>-6 </x-m>
      <y-m> -0.62 </y-m>
      <z-m> -0.8 </z-m>
    </offsets>
  </model>
  
  <!-- Pilot invisible internally -->
  <animation>
    <type>select</type>
    <object-name>Pilot1</object-name>
	<object-name>Pilot2</object-name>
    <condition>
     
        <property>sim/current-view/view-number</property>
        <value>0</value>
     
    </condition>
  </animation>
  
  
   <!-- cyclic + Collective -->
  
    <model>
    <path>Aircraft/UH-60/Models/Interior/sticks/cyclic.xml</path>
    <offsets>
      <x-m> -6.22 </x-m>
      <y-m>  0.6700 </y-m>
      <z-m> -1.32 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>
  
   <model>
    <path>Aircraft/UH-60/Models/Interior/sticks/cyclic.xml</path>
    <offsets>
       <x-m> -6.22 </x-m>
      <y-m>  -0.6600 </y-m>
      <z-m> -1.32 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>
  
   <model>
    <path>Aircraft/UH-60/Models/Interior/sticks/collective.xml</path>
    <offsets>
      <x-m> -5.79 </x-m>
      <y-m>  0.335 </y-m>
      <z-m> -1.22 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>
  
  <model>
    <path>Aircraft/UH-60/Models/Interior/sticks/collective.xml</path>
    <offsets>
      <x-m> -5.79 </x-m>
      <y-m>  -1 </y-m>
      <z-m> -1.22 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

 
<!-- main dash --> 
 
<model>
    <name>panel2</name>
    <path>Aircraft/UH-60/Models/Interior/panel2.xml</path>
    <offsets>
      <x-m>-0.729</x-m>
      <y-m>0</y-m>
      <z-m>2.79</z-m>
	  <pitch-deg>-27</pitch-deg>
<heading-deg>0</heading-deg>
    </offsets>
	
  </model>
  
  <!-- Centre consol --> 
 
<model>
    <name>Centre</name>
    <path>Aircraft/UH-60/Models/Interior/Panels/console2.xml</path>
    <offsets>
      <x-m>-6.48</x-m>
      <y-m>0.05</y-m>
      <z-m>-0.985</z-m>
	  <pitch-deg>12</pitch-deg>
<heading-deg>0</heading-deg>
    </offsets>
	
  </model>
 
 
  <!-- overhead consol --> 
 
<model>
    <name>overhead</name>
    <path>Aircraft/UH-60/Models/Interior/Panels/overhead.xml</path>
    <offsets>
      <x-m>-6.1947</x-m>
      <y-m>0.05</y-m>
      <z-m>-0.135</z-m>
	  <pitch-deg>90</pitch-deg>
<heading-deg>0</heading-deg>
    </offsets>
	<overlay>
		 <animation n="100">
    <type>scale</type>
    <property>constants/zero</property>
    <x-offset>0.85</x-offset>
    <y-offset>0.8</y-offset>
    <z-offset>1</z-offset>
   </animation>
  </overlay>
  </model>

  <model> 
    <path>Aircraft/UH-60/Models/Interior/Panels/Instruments/garmin196/garmin196_support1.xml</path>
    <offsets> 
      <x-m>-6.370</x-m>
      <y-m>0.065</y-m>
      <z-m>-0.4333</z-m>
    <pitch-deg>-15</pitch-deg>
<heading-deg>0</heading-deg>
    </offsets>
  </model>

</PropertyList>

