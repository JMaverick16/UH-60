<?xml version="1.0"?>

<PropertyList>

    <path>knob.ac</path>
    
    <animation>
       <type>range</type>
       <min-m>0</min-m>
       <max-m>5</max-m>
    </animation>


  <animation>
    <type>material</type>
     <object-name>freq.knob</object-name>
    <emission>
      <red>   0.5 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
    </emission>
  </animation>


 

</PropertyList>


