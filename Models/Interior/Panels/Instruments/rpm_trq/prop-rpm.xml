<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>prop-rpm.ac</path>

    <animation>
        <type>material</type>
        <object-name>rpm1</object-name>
        <object-name>rpm2</object-name>
        <object-name>rpmR</object-name>
        <object-name>trq1</object-name>
        <object-name>trq2</object-name>
     <emission>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
  </animation>

    <animation>
        <type>material</type>
        <object-name>digit10R</object-name>
        <object-name>digit1R</object-name>
        <object-name>digit10L</object-name>
        <object-name>digit1L</object-name>
     <emission>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
  </animation>

    <animation>
        <type>material</type>
        <object-name>Cube</object-name>
     <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
      <red>   0.5 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

