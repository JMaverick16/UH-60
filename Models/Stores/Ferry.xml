<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
<!--Ferry Configuration  -->

 <!-- wings -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/wings.xml</path>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>

  <!-- Left outer tank -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/droptank.xml</path>
    <offsets>
      <x-m>-3.5</x-m>
      <y-m>-3</y-m>
      <z-m>-.9</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>
  
  
  <!-- Left inner tank -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/droptank-large.xml</path>
    <offsets>
      <x-m>-3.5</x-m>
      <y-m>-2.1</y-m>
      <z-m>-1.03</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>
  
  
  <!-- Right inner tank -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/droptank-large.xml</path>
    <offsets>
      <x-m>-3.5</x-m>
      <y-m>2.1</y-m>
      <z-m>-1.03</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>
  
  
  
   <!-- Right outer tank -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/droptank.xml</path>
    <offsets>
      <x-m>-3.5</x-m>
      <y-m>3</y-m>
      <z-m>-.9</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>

</PropertyList>
