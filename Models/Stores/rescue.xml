<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
<!--casr Configuration Extended range -->

 <!-- minigun R -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/minigun/minigun.xml</path>
    <offsets>
      <x-m>-4.8</x-m>
      <y-m>1.6</y-m>
      <z-m>-0.8</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>-45</heading-deg>
    </offsets>
  </model>
  
   <!-- minigun ammo R -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/ammotin.xml</path>
    <offsets>
     <x-m>-4.2</x-m>
      <y-m>0.9</y-m>
      <z-m>-1.3</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>

 <!-- minigun L -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/minigun/minigun2.xml</path>
    <offsets>
      <x-m>-4.8</x-m>
      <y-m>-1.6</y-m>
      <z-m>-0.8</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>-45</heading-deg>
    </offsets>
  </model>
  
   <!-- minigun ammo L -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/ammotin2.xml</path>
    <offsets>
     <x-m>-4.2</x-m>
      <y-m>-0.9</y-m>
      <z-m>-1.3</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>

  
   <!-- Internal fuel pods -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/auxfuel.xml</path>
    <offsets>
     <x-m>-1.9</x-m>
      <y-m>0</y-m>
      <z-m>-1</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>180</heading-deg>
    </offsets>
  </model>
  
  
   <!-- refueling probe -->
  <model>
    <path>Aircraft/UH-60/Models/Stores/fuelprobe/fuelprobe.xml</path>
    <offsets>
     <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0.15</z-m>
      <roll-deg>0</roll-deg>
      <pitch-deg>0</pitch-deg>
      <heading-deg>0</heading-deg>
    </offsets>
  </model>
 
 <!-- winch -->

  <model>
    <path>Aircraft/UH-60/Models/Stores/winch/winch.xml</path>
    <offsets>
      <x-m>-1.75</x-m>
      <y-m>-0.25</y-m>
      <z-m>-0.4</z-m>
    </offsets>
  </model>  
  
  
   <model>
      <name>Rescue-Op</name>
      <path>Aircraft/UH-60/Models/Stores/Rescue/Rescue-Op.xml</path>
      <offsets>
        <x-m>-1.75</x-m>
        <y-m>-0.25</y-m>
        <z-m>-0.4</z-m>
      </offsets>
   </model>
    
<!-- crew -->
  <model>
    <name>Rescue-Man</name>
    <path>Aircraft/UH-60/Models/Stores/Rescue/Rescue-Man.xml</path>
    <offsets>
      <x-m>-1.0</x-m>
      <y-m>0.7</y-m>
      <z-m>-0.35</z-m>
    </offsets>
  </model>

</PropertyList>