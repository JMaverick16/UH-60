		<door>
			<name>open and close passenger door start inside</name>
			<mode>once</mode>
			<path>Aircraft/UH-60/Sounds/door_slide_close_end.wav</path>
			<condition>
				<less-than>
					<property>instrumentation/doors/passenger/position-norm</property>
					<value>0.02</value>
				</less-than>
			</condition>
			<volume>
				<property>sound/view-level-inside</property>
			</volume>
		</door>

		<door>
			<name>open and close passenger door end inside</name>
			<mode>once</mode>
			<path>Aircraft/UH-60/Sounds/door_slide_open_start.wav</path>
			<condition>
				<greater-than>
					<property>instrumentation/doors/passenger/position-norm</property>
					<value>0.99</value>
				</greater-than>
			</condition>
			
			<volume>
				<property>sound/view-level-inside</property>
			</volume>
		</door>
		<door>
			<name>open and close passenger door INSIDE</name>
