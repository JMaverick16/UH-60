<?xml version="1.0"?>
<!--
	thanks to the SAR simulation (GPL) for the helicopter sounds,
	and the Urban game (LGPL) for the weapon sounds

	+x = aft
	+y = left
	+z = up
-->

<PropertyList>
	<fx>

		<!-- AVIONICS - inside the cockpit  -->
		<avionics>
			<name>avionics1</name>
			<mode>looped</mode>
			<path>Aircraft/UH-60/Sounds/avionics_1.wav</path>
			<volume>
				<factor>0.06</factor>
			</volume>
			<volume>
				<property>sound/view-level-inside</property>
			</volume>
			<position>
				<x>-1</x>
				<y>0.40</y>
				<z>0.5</z>
			</position>
		</avionics>
		<avionics>
			<name>avionics2</name>
			<mode>looped</mode>
			<path>Aircraft/UH-60/Sounds/avionics_2.wav</path>
			<volume>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>sound/view-level-inside</property>
			</volume>
			<position>
				<x>-1</x>
				<y>-0.40</y>
				<z>0.5</z>
			</position>
		</avionics>
		<avionics>
			<name>avionics3</name>
			<mode>looped</mode>
			<path>Aircraft/UH-60/Sounds/avionics_3.wav</path>
			<volume>
				<factor>0.03</factor>
			</volume>
			<volume>
				<property>sound/view-level-inside</property>
			</volume>
			<position>
				<x>0</x>
				<y>0</y>
				<z>0</z>
			</position>
		</avionics>
		<!-- AVIONICS - outside the cockpit  -->
		<avionics>
			<name>avionics-outside</name>
			<mode>looped</mode>
			<path>Aircraft/UH-60/Sounds/auxiliry_outside.wav</path>
			<volume>
				<factor>0.03</factor>
			</volume>
			<volume>
				<property>sound/view-level-outside</property>
			</volume>
			<position>
				<x>0</x>
				<y>0</y>
				<z>0</z>
			</position>
			<reference-dist>10</reference-dist>
			<max-dist>100</max-dist>
		</avionics>

		<!-- Starting Engines -->


		<starter>
			<name>starter0_start</name>
			<mode>once</mode>
			<path>Aircraft/UH-60/Sounds/uh60_full_startup_sequence.wav</path>
			<condition>
				<property>procedures/startup-running</property>
			</condition>
			<volume>
				<factor>1</factor>
			</volume>
			<volume>
				<property>sound/view-level-outside</property>
			</volume>
			<reference-dist>10</reference-dist>
			<max-dist>5000</max-dist>
		</starter>

		<engine>
			<name>turbine1-loop</name>
			<mode>looped</mode>
			<path>Sounds/uh60_turbine_loop.wav</path>
			<condition>
				<property>engines/engine[0]/running</property>
			</condition>
			<volume>
				<property>engines/engine[0]/n2-pct</property>
				<factor>0.1</factor>
			</volume>
			<volume>
				<property>sound/view-level-outside</property>
			</volume>
			<pitch>
				<property>engines/engine[0]/n2-pct</property>
				<factor>0.0000309</factor>
			</pitch>
			<reference-dist>10</reference-dist>
			<max-dist>1000</max-dist>
		</engine>

		<engine>
			<name>turbine2-loop</name>
			<mode>looped</mode>
			<path>Sounds/uh60_turbine_loop.wav</path>
			<condition>
				<property>engines/engine[1]/running</property>
			</condition>
			<volume>
				<property>engines/engine[1]/n2-pct</property>
				<factor>0.1</factor>
			</volume>
			<volume>
				<property>sound/view-level-outside</property>
			</volume>
			<pitch>
				<property>engines/engine[1]/n2-pct</property>
				<factor>0.0000309</factor>
			</pitch>
			<reference-dist>10</reference-dist>
			<max-dist>1000</max-dist>
		</engine>


		<starter-inside>
